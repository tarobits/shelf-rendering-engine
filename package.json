{
  "name": "@tarobits/shelf-rendering-engine",
  "version": "0.1.5",
  "description": "An engine for rending shelves with Vue.js",
  "license": "MPL-2.0",
  "author": {
    "name": "Tarobits"
  },
  "type": "module",
  "main": "./dist/shelf-rendering-engine.umd.cjs",
  "module": "./dist/shelf-rendering-engine.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/shelf-rendering-engine.js",
      "require": "./dist/shelf-rendering-engine.umd.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
